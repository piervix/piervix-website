{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build:css": "postcss ./src/styles/main.css -o ./src/styles/index.css",
    "watch:css": "postcss ./src/styles/main.css -o ./src/styles/index.css -w",
    "build": "npm run build:css & gatsby build",
    "develop": "npm run watch:css & env-cmd -f ./.env.development gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "exports-loader": "^0.7.0",
    "gatsby": "^2.19.7",
    "gatsby-plugin-manifest": "^2.2.41",
    "gatsby-plugin-offline": "^3.0.34",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-source-graphql": "^2.1.33",
    "gsap": "^3.1.1",
    "lethargy": "^1.0.9",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.7.4",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "prettier": "^1.19.1",
    "tailwindcss": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
